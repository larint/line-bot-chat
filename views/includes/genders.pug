table
    thead
        tr
            td(colspan=4) 
                h5 Percentage per gender
        tr
            th Date Update
            th UnKnown
            th Male
            th Female
    tbody
        each item in genders
            tr
                td=item.date_update
                td=item.unknown+'%'
                td=item.male+'%'
                td=item.female+'%'