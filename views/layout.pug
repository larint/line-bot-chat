doctype html
html
  head
    title= title
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.1/css/bootstrap.min.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/stylesheets/jquery.notify.css')
    link(rel='stylesheet', href='/stylesheets/jquery.notify.fonts.css')
  body
    block content
    script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js")
    script(src="javascripts/jquery.notify.js")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.2.0/socket.io.js")
    script.
      var socket = io('!{site_url}')

      socket.on('schedule_get_line_data', function(msg){
          $.notify(msg.message);
      });

      